{
  "name": "FirePHP for Chrome",
  "version": "0.1",
  "minimum_chrome_version": "17.0",
  "description": "Adding support for parsing FirePHP messages from server",
  "background_page": "Html/background.html",
  "browser_action": {
      "default_icon": "Images/icon_small.png"
    },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
